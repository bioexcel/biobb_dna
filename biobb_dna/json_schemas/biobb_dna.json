{
    "_id": "biobb_dna",
    "desc": "Biobb_dna is a package composed of different analyses for nucleic acid trajectories",
    "github": "https://github.com/bioexcel/biobb_dna",
    "readthedocs": "https://biobb-dna.readthedocs.io/en/latest/",
    "conda": "",
    "docker": "",
    "singularity": "",
    "version": "2.0.0",
    "tools": [
        {
            "block": "curvesplus",
            "tool": "Curves",
            "desc": "Wrapper for the Cur+ program that is part of the Curves+ software suite.",
            "exec": "biobb_curves",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "curvesplus",
            "tool": "Canal",
            "desc": "Wrapper for the Canal program that is part of the Curves+ software suite.",
            "exec": "biobb_canal",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "backbone",
            "tool": "BI/BII Populations",
            "desc": "Computation of BI/BII populations for the epsil/zeta helical parameters.",
            "exec": "bipopulations",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "backbone",
            "tool": "Canonical Alpha-Gamma",
            "desc": "Calculate Canonical Alpha/Gamma populations from alpha and gamma parameters.",
            "exec": "canonicalag",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "backbone",
            "tool": "Puckering",
            "desc": "Calculate Puckering from phase parameters.",
            "exec": "puckering",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "dna",
            "tool": "averages",
            "desc": "Load .ser file for a given helical parameter and read each column corresponding to a base calculating average over each one.",
            "exec": "dna_averages",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "dna",
            "tool": "timeseries",
            "desc": "Created time series and histogram plots for each base pair from a helical parameter series file.",
            "exec": "dna_timeseries",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "dna",
            "tool": "bimodality",
            "desc": "Determine binormality/bimodality from a helical parameter series dataset.",
            "exec": "dna_bimodality",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "stiffness",
            "tool": "average_stiffness",
            "desc": "Calculate average stiffness constants for each base pair of a trajectory's series.",
            "exec": "avgstiff",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "stiffness",
            "tool": "basepair_stiffness",
            "desc": "Calculate stiffness constants matrix between all six helical parameters for a single base pair step.",
            "exec": "bpstiff",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "interbp_correlations",
            "tool": "basepaircorrelation",
            "desc": "Calculate correlation between all base pairs of a single sequence and for a single helical parameter.",
            "exec": "inter_bpcorr",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "interbp_correlations",
            "tool": "helparcorrelation",
            "desc": "Calculate correlation between helical parameters for a single inter-base pair.",
            "exec": "inter_hpcorr",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "interbp_correlations",
            "tool": "sequencecorrelation",
            "desc": "Calculate correlation between all base pairs of a single sequence and for a single helical parameter.",
            "exec": "inter_seqcorr",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "intrabp_correlations",
            "tool": "basepaircorrelation",
            "desc": "Calculate correlation between all intra-base pairs of a single sequence and for a single helical parameter.",
            "exec": "intra_bpcorr",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "intrabp_correlations",
            "tool": "helparcorrelation",
            "desc": "Calculate correlation between helical parameters for a single intra-base pair.",
            "exec": "intra_hpcorr",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        },
        {
            "block": "intrabp_correlations",
            "tool": "sequencecorrelation",
            "desc": "Calculate correlation between all intra-base pairs of a single sequence and for a single helical parameter.",
            "exec": "intra_seqcorr",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna"
        }
    ],
    "dep_pypi": [
        "install_requires=['biobb_common==3.5.1']",
        "python_requires='==3.7.*'",
        "pandas>=1.2",
        "matplotlib>=3.3",
        "numpy>=1.20",
        "scikit-learn>=0.24"
    ],
    "dep_conda": [
        "python ==3.7.*",
        "biobb_common ==3.5.1",
        "nb_conda_kernels",
        "nose",
        "conda",
        "curves",
        "pytraj",
        "pandas>=1.2",
        "matplotlib>=3.3",
        "numpy>=1.20",
        "scikit-learn>=0.24"
    ],
    "keywords": [
        "dna",
        "curves",
        "canal"
    ]
}