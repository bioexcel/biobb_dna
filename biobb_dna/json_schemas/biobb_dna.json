{
    "_id": "biobb_dna",
    "desc": "Biobb_dna is a package composed of different analyses for nucleic acid trajectories.",
    "github": "https://github.com/bioexcel/biobb_dna",
    "readthedocs": "https://biobb-dna.readthedocs.io/en/latest/",
    "conda": "https://anaconda.org/bioconda/biobb_dna",
    "docker": "https://quay.io/biocontainers/biobb_dna:3.7.1--pyhdfd78af_0",
    "singularity": "",
    "version": "3.7.1",
    "tools": [
        {
            "block": "Curves",
            "tool": "Curves",
            "desc": "Wrapper for the Cur+ program that is part of the Curves+ software suite.",
            "exec": "biobb_curves",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/curvesplus.html#module-curvesplus.biobb_curves"
        },
        {
            "block": "Canal",
            "tool": "Canal",
            "desc": "Wrapper for the Canal program that is part of the Curves+ software suite.",
            "exec": "biobb_canal",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/curvesplus.html#module-curvesplus.biobb_canal"
        },
        {
            "block": "Canion",
            "tool": "Canion",
            "desc": "Wrapper for the Canion executable  that is part of the Curves+ software suite. ",
            "exec": "biobb_canion",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/curvesplus.html#module-curvesplus.biobb_canion"
        },
        {
            "block": "BIPopulations",
            "tool": "In House",
            "desc": "Computation of BI/BII populations for the epsil/zeta helical parameters.",
            "exec": "bipopulations",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/backbone.html#module-backbone.bipopulations"
        },
        {
            "block": "CanonicalAG",
            "tool": "In House",
            "desc": "Calculate Canonical Alpha/Gamma populations from alpha and gamma parameters.",
            "exec": "canonicalag",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/backbone.html#module-backbone.canonicalag"
        },
        {
            "block": "Puckering",
            "tool": "In House",
            "desc": "Calculate Puckering from phase parameters.",
            "exec": "puckering",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/backbone.html#module-backbone.puckering"
        },
        {
            "block": "HelParAverages",
            "tool": "In House",
            "desc": "Load .ser file for a given helical parameter and read each column corresponding to a base calculating average over each one.",
            "exec": "dna_averages",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna_averages"
        },
        {
            "block": "HelParTimeSeries",
            "tool": "In House",
            "desc": "Created time series and histogram plots for each base pair from a helical parameter series file.",
            "exec": "dna_timeseries",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna_timeseries"
        },
        {
            "block": "HelParBimodality",
            "tool": "In House",
            "desc": "Determine binormality/bimodality from a helical parameter series dataset.",
            "exec": "dna_bimodality",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/dna.html#module-dna.dna_bimodality"
        },
        {
            "block": "AverageStiffness",
            "tool": "In House",
            "desc": "Calculate average stiffness constants for each base pair of a trajectory's series.",
            "exec": "average_stiffness",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/stiffness.html#module-stiffness.average_stiffness"
        },
        {
            "block": "BPStiffness",
            "tool": "In House",
            "desc": "Calculate stiffness constants matrix between all six helical parameters for a single base pair step.",
            "exec": "basepair_stiffness",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/stiffness.html#module-stiffness.basepair_stiffness"
        },
        {
            "block": "InterBasePairCorrelation",
            "tool": "In House",
            "desc": "Calculate correlation between all base pairs of a single sequence and for a single helical parameter.",
            "exec": "interbpcorr",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/interbp_correlations.html#interbp-correlations-interbpcorr-module"
        },
        {
            "block": "InterHelParCorrelation",
            "tool": "In House",
            "desc": "Calculate correlation between helical parameters for a single inter-base pair.",
            "exec": "interhpcorr",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/interbp_correlations.html#interbp-correlations-interhpcorr-module"
        },
        {
            "block": "InterSequenceCorrelation",
            "tool": "In House",
            "desc": "Calculate correlation between all base pairs of a single sequence and for a single helical parameter.",
            "exec": "interseqcorr",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/interbp_correlations.html#interbp-correlations-interseqcorr-module"
        },
        {
            "block": "IntraBasePairCorrelation",
            "tool": "In House",
            "desc": "Calculate correlation between all intra-base pairs of a single sequence and for a single helical parameter.",
            "exec": "intrabasepaircorrelation",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/intrabp_correlations.html#intrabp-correlations-intrabpcorr-module"
        },
        {
            "block": "IntraHelParCorrelation",
            "tool": "In House",
            "desc": "Calculate correlation between helical parameters for a single intra-base pair.",
            "exec": "intrahelparcorrelation",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/intrabp_correlations.html#intrabp-correlations-intrahpcorr-module"
        },
        {
            "block": "IntraSequenceCorrelation",
            "tool": "In House",
            "desc": "Calculate correlation between all intra-base pairs of a single sequence and for a single helical parameter.",
            "exec": "intrasequencecorrelation",
            "docs": "https://biobb-dna.readthedocs.io/en/latest/intrabp_correlations.html#module-intrabp_correlations.intraseqcorr"
        }
    ],
    "dep_pypi": [
        "install_requires=['biobb_common==3.7.0']",
        "python_requires='==3.7.*'",
        "pandas>=1.2",
        "matplotlib>=3.4",
        "numpy>=1.20",
        "scikit-learn>=0.24"
    ],
    "dep_conda": [
        "python ==3.7.*",
        "biobb_common ==3.7.0",
        "curves",
        "pandas>=1.2",
        "matplotlib>=3.4",
        "numpy>=1.20",
        "scikit-learn>=0.24"
    ],
    "keywords": [
        "dna",
        "curves",
        "canal"
    ]
}